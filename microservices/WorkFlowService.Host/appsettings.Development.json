{
  "AuthServer": {
    "Authority": "https://localhost:44399",
    "RequireHttpsMetadata": "true",
    "ApiName": "FavaWorkflowApi"
  },
  "ConnectionStrings": {
    "WorkflowManagement": "Server=db-Workflow;Database=Esale_Payment;Integrated Security=SSPI;Persist Security Info=True;Trusted_Connection=True;TrustServerCertificate=true",
    "AbpAuditLogging": "Server=db-dev;Database=Esale_AuditLog;Integrated Security=SSPI;Persist Security Info=True;Trusted_Connection=True;TrustServerCertificate=true"
  },
  "Redis": {
    "Configuration": "localhost:6379"
  },
