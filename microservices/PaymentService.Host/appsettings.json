{
  "AuthServer": {
    "Authority": "https://localhost:44399",
    "RequireHttpsMetadata": "true",
    "ApiName": "FavaPaymentApi"
  },
  "ConnectionStrings": {
    "PaymentManagement": "Server=db-dev;Database=PaymentManagement;Integrated Security=SSPI;Persist Security Info=True;Trusted_Connection=True;TrustServerCertificate=true"
  },
  "App": {
    "RetryCount": 10,
    "IranKishCallBackUrl": "https://localhost:44344/api/services/app/PaymentService/BackFromIranKish",
    "MellatCallBackUrl": "https://localhost:44344/api/services/app/PaymentService/BackFromMellat"

  },
  "Redis": {
    "Configuration": "localhost:6379"
  },
  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost"
      }
    },
    "EventBus": {
      "ClientName": "MsDemo_ProductService",
      "ExchangeName": "MsDemo"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
